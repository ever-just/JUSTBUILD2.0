{
  "framework": "nextjs",
  "buildCommand": "cd ../.. && yarn workspace @open-swe/shared build && yarn workspace @open-swe/web build",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && yarn install --immutable || yarn install --no-immutable",
  "devCommand": "yarn workspace @open-swe/web dev",
  "rootDirectory": "apps/web",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "env": {
    "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
  }
}